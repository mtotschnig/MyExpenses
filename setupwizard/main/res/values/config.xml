<resources>

    <!-- The duration (in milliseconds) of activity transitions -->
    <integer name="suwTransitionDuration">300</integer>

    <!-- Whether to use tablet layout -->
    <bool name="suwUseTabletLayout">false</bool>

    <!-- ID used with setTag(int, Object) to store the original weight of a view inside
         ButtonBarLayout -->
    <item name="suw_original_weight" type="id" />

    <!-- Secondary font for use with headings, title, and other non-body text -->
    <string name="suwFontSecondary" translatable="false">google-sans</string>
    <string name="suwFontSecondaryMedium" translatable="false">google-sans-medium</string>

</resources>
